{
    "diagnostics": {
        "globals": [
            "vim"
        ]
    },
    "runtime": {
        "path": [
            "?.lua",
            "?/init.lua"
        ],
        "version": "LuaJIT"
    },
    "workspace": {
        "checkThirdParty": false,
        "ignoreDir": [
            ".git",
            ".github",
            "node_modules",
            ".local/share/nvim/site"
        ],
        "library": [
            "${3rd}/luv/library",
            "${3rd}/busted/library",
            "${3rd}/luassert/library"
        ]
    }
}
